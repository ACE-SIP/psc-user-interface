{{>header}}

<body class="g-sidenav-show bg-gray-100">
<main class="main-content max-height-vh-100 h-100">
    <div class="container-fluid my-3 py-3">
        <div class="row mb-5">
            <div class="col-lg-9 mt-lg-0 mt-4">
                <!-- Card Basic Info -->
                <div class="card mt-4" id="basic-info">
                    <div class="card-header">
                        <h5>Basic Device Info</h5>
                    </div>
                    <div class="card-body pt-0">
                        <div class="row">
                            <div class="col-6">
                                <label class="form-label">Device Name</label>
                                <div class="input-group">
                                    <input id="firstName" name="firstName" class="form-control" type="text"
                                           placeholder="" required="required">
                                </div>
                            </div>
                            <div class="col-6">
                                <label class="form-label">Product Id</label>
                                <div class="input-group">
                                    <input id="lastName" name="lastName" class="form-control" type="text"
                                           placeholder="" required="required">
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-6">
                                <label class="form-label mt-4">Channel Id</label>
                                <div class="input-group">
                                    <input id="email" name="" class="form-control" type="number"
                                           placeholder="">
                                </div>
                            </div>
                            <div class="col-6">
                                <label class="form-label mt-4">Confirmation Channel Id</label>
                                <div class="input-group">
                                    <input id="confirmation" name="confirmation" class="form-control" type="number"
                                           placeholder="">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-6">
                                <label class="form-label mt-4">Product Provider</label>
                                <select class="form-control" name="choices-gender" id="choices-gender">
                                    <option value="Male">ubibot</option>
                                    <option value="Female">Others</option>
                                </select>
                            </div>
                            <div class="col-6">
                                <label class="form-label mt-4">Serial No</label>
                                <select class="form-control" name="choices-serial" id="choices-serial">
                                    <option value="WS1">WS1</option>
                                    <option value="WS1-PRO">WS1-PRO</option>
                                    <option value="GS1">GS1</option>
                                    <option value="GS2">GS2</option>
                                    <option value="MS1">MS1</option>
                                </select>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-6">
                                <div class="row">
                                    <div class="col-sm-5 col-2">
                                        <label class="form-label mt-4">Valid From</label>
                                        <select class="form-control" name="choices-month" id="choices-month"></select>
                                    </div>
                                    <div class="col-sm-3 col-2">
                                        <label class="form-label mt-4">&nbsp;</label>
                                        <select class="form-control" name="choices-day" id="choices-day"></select>
                                    </div>
                                    <div class="col-sm-4 col-5">
                                        <label class="form-label mt-4">&nbsp;</label>
                                        <select class="form-control" name="choices-year" id="choices-year"></select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-6">
                                <div class="row">
                                    <div class="col-sm-5 col-3">
                                        <label class="form-label mt-4">Expire Until</label>
                                        <select class="form-control" name="choices-month" id="choices-month"></select>
                                    </div>
                                    <div class="col-sm-3 col-3">
                                        <label class="form-label mt-4">&nbsp;</label>
                                        <select class="form-control" name="choices-day" id="choices-day"></select>
                                    </div>
                                    <div class="col-sm-4 col-3">
                                        <label class="form-label mt-4">&nbsp;</label>
                                        <select class="form-control" name="choices-year" id="choices-year"></select>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <button class="btn bg-gradient-dark btn-sm float-end mt-6 mb-0">Register Device</button>

                    </div>
                </div>
            </div>
        </div>
        <footer class="footer pt-3  ">
            <div class="container-fluid">
                <div class="row align-items-center justify-content-lg-between">
                    <div class="col-lg-6 mb-lg-0 mb-4">
                        <div class="copyright text-center text-sm text-muted text-lg-start">
                            Â©
